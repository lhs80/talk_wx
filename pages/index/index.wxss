page{
  background-color: rgb(255, 255, 255);
}
.login{
  width: 100%;
  height: 100vh;
  display: flex;
  justify-content: center;
  align-items: center;
}
.login_from{
  width: 95%;
  margin: 0 auto;
  
}
.logo{
  text-align: center;
  margin-top: calc(-100vmin / 7.5);
  margin-bottom: calc(100vmin / 7.5)
}
.title{
  font-size: calc(35vmin / 7.5);
  font-weight: bold;
}
.warn_msg{
  padding: calc(20vmin / 7.5);
  font-size: calc(23vmin / 7.5);
  color: rgb(179, 179, 179);
}
.login_input,.login_input_label{
  height: calc(80vmin / 7.5);
  font-size: calc(30vmin / 7.5);
  line-height: calc(80vmin / 7.5);
}
.submitBtn{
  width: 95% !important;
  margin-top: calc(50vmin / 7.5);
  font-size: calc(35vmin / 7.5) !important;
  padding: calc(50vmin / 7.5) !important;
  border-radius: calc(100vmin / 7.5) !important;
}
.copy{
  position: fixed;
  left: 10vw;
  right: 10vw;
  bottom: calc(50vmin / 7.5);
  width: 80vw;
  text-align: center;
  font-size: calc(22vmin / 7.5);
  color: rgb(143, 143, 143);
  margin-top: calc(50vmin / 7.5);
}
