.water_top{
  position: fixed;
  top:0;
  bottom: 0;
  left: 0;
  right: 0;
  width: 100%;
  height: 100%;
  z-index: 9999;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.water-text{
  width: calc(100% / 5);
  color: rgba(169,169,169,.2);
  text-align: center;
  font-size: 95%;
  transform:rotate(-40deg);
}
